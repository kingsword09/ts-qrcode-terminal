{
  "name": "@kingsword09/ts-qrcode-terminal",
  "version": "1.0.0",
  "exports": "./index.ts",
  "tasks": {
    "build": "deno run -A ./scripts/npm.ts",
    "pub": "deno run --unstable -A ./scripts/pub.ts",
    "test": "deno run -A ./examples/test.ts"
  },
  "compilerOptions": {
    "allowJs": false,
    "lib": [
      "deno.unstable",
      "deno.window",
      "DOM"
    ],
    "strict": true,
    "strictNullChecks": true
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-namespace"
      ]
    }
  },
  "fmt": {
    "options": {
      "useTabs": false,
      "lineWidth": 120,
      "indentWidth": 2,
      "singleQuote": false,
      "proseWrap": "preserve"
    }
  },
  "imports": {
    "dnt": "jsr:@deno/dnt",
    "std_path": "jsr:@std/path"
  },
  "exclude": ["scripts"]
}
